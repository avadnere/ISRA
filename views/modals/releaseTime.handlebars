<div class="modal-header">
   <button type="button" class="close" data-dismiss="modal">&times;</button>
   <h4 class="modal-title">Add Release Time</h4>
</div>
<form data-action="/modals/createReleaseTIme" id="createReleaseTime" method="POST" name="assignmentAjaxForm">
   <div class="modal-body">
      <div class="form-group">
         <label for="Select Course">Select Assignment</label>
         <select class="form-control" name="releaseTimeType" id="releaseTimeType">
            {{#each releaseTimeTypeList}}
            <option value="{{this.releaseTimeTypeId}}">{{this.releaseTimeTypeId}}</option>
            {{/each}}
         </select>
      </div>
      <div class="form-group">
         <div class="row">
            <div class="col-sm-12">
               <label for="tch">TCH</label>
               <input type="number" class="form-control" id="TCH" name="TCH" placeholder="TCH">
            </div>
            {{!-- <div class="col-sm-6">
               <label for="overload">overload</label>
               <input type="number" class="form-control" name="overload" id="overload" value="10" disabled>
            </div> --}}
         </div>
      </div>
      <div class="form-group">
         <label for="description">Description</label>
         <div><textarea name="description" id="description"></textarea></div>
      </div>
      <input type="hidden" value="{{releaseTimeId}}" name="releaseTimeId" id="releaseTimeId">
   </div>
   <div class="modal-footer">
      <button type="submit" class="btn btn-default btn-success">Add</button>
      <button type="button" class="btn btn-default btn-danger" data-dismiss="modal">Close</button>
   </div>
</form>
<script src="../../public/js/script.js"></script>"