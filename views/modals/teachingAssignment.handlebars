<form  data-action="/modals/createTeachingAssignment"  id="createTeachingAssignmentForm" method="POST"  name="assignmentAjaxForm">
<div class="modal-header">
   <button type="button" class="close" data-dismiss="modal">&times;</button>
   <h4 class="modal-title">Add Teaching Assignment</h4>
</div>
<div class="modal-body">
   <form>
      <div class="form-group">
         <div class="row">
            <div class="col-sm-7">
               <label for="courseTypeId">Course</label>
               <select class="form-control" name="courseTypeId" id="courseTypeId">
                    <option value="">Select Course</option>
                  {{#each courseList}}
                  <option value="{{this.courseNumber}}">{{this.courseNumber}} {{this.courseTypeId}}</option>
                  {{/each}}
               </select>
            </div>
             <div class="col-sm-3">
               <label for="sectionTypeId">Section</label>
               <select class="form-control" name="sectionTypeId" id="sectionTypeId">
               </select>
            </div>
            <div class="col-sm-2">
                <label for="sectionTypeId">Schedule</label><br>
              <button id="getSchedule" class="btn btn-default"> <span class="glyphicon glyphicon-calendar"></span>
              </button>
            </div>
         </div>
      </div>
      <div class="form-group courseSchedule-block hide">
         <label for="scheduleTypeId">Schedule </label>
         <table class="table">
            <thead>
               <tr>
                  <th>Course</th>
                  <th>Section</th>
                  <th>Schedule</th>
                  <th>Time</th>
                  <th>Location</th>
               </tr>
            </thead>
            <tbody>
               <tr id="courseSchedule">
                 
               </tr>
            </tbody>
           
         </table>
         <input type="hidden" class="form-control" name="scheduleTypeId" id="scheduleTypeId" >
      </div>
      <div class="form-group">
         <label for="enrollement">Enrollement </label>
         <input type="number" class="form-control" name="enrollement" id="enrollement" placeholder="Enrollement">
      </div>
      <div class="form-group">
         <div class="row">
            <div class="col-sm-4">
               <label for="semhrs">Sem Hours</label>
               <input type="number" class="form-control" name="semHours" id="semHours" placeholder="Sem Hours">
            </div>
            <div class="col-sm-4">
               <label for="ssh">SSH</label>
                <input type="number" class="form-control" id="ssh" placeholder="SSH" disabled>
            </div>
            <div class="col-sm-4">
               <label for="classType">class Type</label>
               <select class="form-control" name="classTypeId" id="classTypeId">

                  {{#each classType}}
                  <option value="{{this.classTypeId}}">{{this.classTypeId}} {{this.classTypeId}}</option>
                  {{/each}}
               </select>
            </div>
         </div>
      </div>
      <div class="form-group">
         <div class="row">
            <div class="col-sm-12">
               <label for="tch">TCH</label>
               <input type="number" class="form-control" name="TCH" id="TCH" placeholder="TCH">
            </div>
            {{!-- <div class="col-sm-6">
               <label for="teachingLoad">Load %</label>
               <input type="number" class="form-control" name="teachingLoad" id="teachingLoad" value="10" disabled>
            </div> --}}
         </div>
      </div>
      <div class="form-group">
         <label for="note">Note</label>
         <div><textarea name="note" id="note"></textarea></div>
      </div>
      <input type="hidden" value={{teachingAssignmentId}} name="teachingAssignmentId" id="teachingAssignmentId">
  
</div>
<div class="modal-footer">
   <button type="submit" class="btn btn-default btn-success">Add</button>
   <button type="button" class="btn btn-default btn-danger" data-dismiss="modal">Close</button>
</div>
</form>
<script src="../../public/js/script.js"></script>